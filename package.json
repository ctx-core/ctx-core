{
	"name": "ctx-core",
	"version": "5.18.8",
	"description": "ctx-core core library",
	"keywords": [
		"ctx-core",
		"array",
		"combinators",
		"function",
		"object",
		"set"
	],
	"homepage": "https://github.com/ctx-core/ctx-core#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/ctx-core/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/ctx-core.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"_",
		"_array",
		"_atob",
		"_base16",
		"_be",
		"_btoa",
		"_buffer",
		"_chain",
		"_class",
		"_cli-args",
		"_color",
		"_combinators",
		"_crypto",
		"_currency",
		"_data",
		"_date",
		"_debounce",
		"_deep_equal",
		"_env",
		"_error",
		"_falsy",
		"_fetch",
		"_fibonacci",
		"_fs",
		"_function",
		"_functional",
		"_html",
		"_http",
		"_math",
		"_matrix",
		"_nullish",
		"_number",
		"_object",
		"_promise",
		"_queue",
		"_random",
		"_regex",
		"_rmemo",
		"_run",
		"_set",
		"_sleep",
		"_string",
		"_tempfile",
		"_test",
		"_time",
		"_tuple",
		"_types",
		"_uri",
		"_uuid",
		"package.json"
	],
	"types": "./src/index.d.ts",
	"exports": {
		".": "./index.js",
		"./all": "./_/index.js",
		"./array": "./_array/index.js",
		"./atob": "./_atob/index.js",
		"./base16": "./_base16/index.js",
		"./be": "./_be/index.js",
		"./btoa": "./_btoa/index.js",
		"./buffer": "./_buffer/index.js",
		"./chain": "./_chain/index.js",
		"./class": "./_class/index.js",
		"./cli-args": "./_cli-args/index.js",
		"./color": "./_color/index.js",
		"./combinators": "./_combinators/index.js",
		"./crypto": "./_crypto/index.js",
		"./currency": "./_currency/index.js",
		"./data": "./_data/index.js",
		"./date": "./_date/index.js",
		"./debounce": "./_debounce/index.js",
		"./deep_equal": "./_deep_equal/index.js",
		"./env": "./_env/index.js",
		"./error": "./_error/index.js",
		"./falsy": "./_falsy/index.js",
		"./fetch": "./_fetch/index.js",
		"./fibonacci": "./_fibonacci/index.js",
		"./fs": "./_fs/index.js",
		"./function": "./_function/index.js",
		"./functional": "./_functional/index.js",
		"./html": "./_html/index.js",
		"./http": "./_http/index.js",
		"./math": "./_math/index.js",
		"./matrix": "./_matrix/index.js",
		"./nullish": "./_nullish/index.js",
		"./number": "./_number/index.js",
		"./object": "./_object/index.js",
		"./promise": "./_promise/index.js",
		"./queue": "./_queue/index.js",
		"./random": "./_random/index.js",
		"./regex": "./_regex/index.js",
		"./rmemo": "./_rmemo/index.js",
		"./run": "./_run/index.js",
		"./set": "./_set/index.js",
		"./sleep": "./_sleep/index.js",
		"./string": "./_string/index.js",
		"./tempfile": "./_tempfile/index.js",
		"./test": "./_test/index.js",
		"./time": "./_time/index.js",
		"./tuple": "./_tuple/index.js",
		"./types": "./_types/index.js",
		"./uri": "./_uri/index.js",
		"./uuid": "./_uuid/index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm _test",
		"test": "pnpm run /^test:/",
		"test:size": "size-limit",
		"test:type": "check-dts",
		"test:unit": "NODE_OPTIONS=--loader=esmock tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:coverage": "c8 pnpm test:unit"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.13.5",
		"@ctx-core/preprocess": "^0.1.1",
		"@size-limit/preset-small-lib": "^11.0.1",
		"@types/node": "^20.11.2",
		"@types/sinon": "^17.0.3",
		"c8": "^9.1.0",
		"check-dts": "^0.7.2",
		"esbuild": "^0.19.11",
		"esmock": "^2.6.2",
		"sinon": "^17.0.1",
		"size-limit": "^11.0.1",
		"tsx": "^4.7.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false,
	"size-limit": [
		{
			"name": "ctx_",
			"import": {
				"./_be": "{ ctx_ }"
			},
			"limit": "33 B"
		},
		{
			"name": "ns_ctx_",
			"import": {
				"./_be": "{ ns_ctx_ }"
			},
			"limit": "85 B"
		},
		{
			"name": "be_",
			"import": {
				"./_be": "{ be_ }"
			},
			"limit": "99 B"
		},
		{
			"name": "be_ ctx_",
			"import": {
				"./_be": "{ be_, ctx_ }"
			},
			"limit": "131 B"
		},
		{
			"name": "be_ ns_ctx_",
			"import": {
				"./_be": "{ be_, ctx_, ns_ctx_ }"
			},
			"limit": "190 B"
		},
		{
			"name": "be_ ctx_ ns_ctx_",
			"import": {
				"./_be": "{ be_, ctx_, ns_ctx_ }"
			},
			"limit": "190 B"
		},
		{
			"name": "memo_",
			"import": {
				"./_rmemo": "{ memo_ }"
			},
			"limit": "352 B"
		},
		{
			"name": "memo_ sig_",
			"import": {
				"./_rmemo": "{ sig_, memo_ }"
			},
			"limit": "370 B"
		},
		{
			"name": "memo_ sig_ be_ ctx_",
			"import": {
				"./_rmemo": "{ sig_, memo_, be_, ctx_ }"
			},
			"limit": "471 B"
		},
		{
			"name": "memo_ sig_ be_ ctx_ be_memo_pair_ be_sig_triple_",
			"import": {
				"./_rmemo": "{ sig_, memo_, be_, ctx_, be_memo_pair_, be_sig_triple_ }"
			},
			"limit": "576 B"
		},
		{
			"name": "uuid",
			"import": {
				"./_uuid": "{ uuid_ }"
			},
			"limit": "39 B"
		},
		{
			"name": "short uuid",
			"import": {
				"./_uuid": "{ short_uuid_ }"
			},
			"limit": "116 B"
		}
	]
}
